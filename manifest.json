{
	"manifest_version": 2,
	"name": "Magic Card Finder",
	"short_name": "Magic Card Finder",
	"description": "Search MTG cards",
	"version": "0.1.0",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"libs/jquery.min.js"
			]
		}
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Magic Card Finder"
	},

	"icons": {
		"128": "icon.png"
	}
}